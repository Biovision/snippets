<% if collection.any? %>
    <ul class="list-of-entities">
      <% collection.each do |order_item| %>
          <li>
            <%= render partial: 'admin/order_items/entity/in_list', locals: { order_item: order_item } %>
          </li>
      <% end %>
    </ul>
<% else %>
    <%= render 'shared/nothing_found' %>
<% end %>
